{
  "name": "react-native-sectionlist-deliveroo",
  "version": "1.0.2",
  "description": "This package is inspired by deliveroo App. It provides a section list with a header component to make the list bidirectionnal.\nThe header is scrolling horizontally and contains the name of the sections.\nWhile scrolling into the section list it updates the header and vice-versa",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "test": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riamon-v/react-native-sectionlist-deliveroo.git"
  },
  "keywords": [
    "deliveroo",
    "header",
    "section-list",
    "section",
    "deliveroo",
    "header-list",
    "header",
    "react-native",
    "scrollview",
    "bidirectionnal list",
    "bidirectionnal"
  ],
  "files": [
    "lib/**/*"
  ],
  "license": "MIT",
  "author": "riamon-v",
  "bugs": {
    "url": "https://github.com/riamon-v/react-native-sectionlist-deliveroo/issues"
  },
  "homepage": "https://github.com/riamon-v/react-native-sectionlist-deliveroo#readme",
  "devDependencies": {
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "metro-react-native-babel-preset": "^0.65.1",
    "prettier": "^2.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "react-native-reanimated": "~2.3.1"
  },
  "peerDependencies": {
    "react": "17.0.1",
    "react-native": "^0.64.3",
    "react-native-reanimated": "~2.3.1"
  }
}
